    "eslint": "^7.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/aframe": "^1.0.3",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "rollup": "^2.38.4",
    "rollup-plugin-dts": "^2.0.1",
    "tslib": "^2.1.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ],
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties"
    ]
  }
